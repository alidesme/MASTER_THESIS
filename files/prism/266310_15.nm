module jam

formula day = (timeOfTheDay <= 8);

formula pick = ((timeOfTheDay > 8) & (timeOfTheDay <= 14));

formula night = ((timeOfTheDay > 14) & (timeOfTheDay <= 24));

[updateJam] (day) -> 1: jamCounter' = jamDay;
[updateJam] (pick) -> 1: jamCounter' = jamPick;
[updateJam] (night) -> 1: jamCounter' = jamNight;
[updateDay] true -> 1: timeOfTheDay' = timeOfTheDay + 1;
formula jam = ((day | pick | night) & jamCounter > 0);


endmodule


module arbiter

token : [0 .. 6] init 0;
[updateJam] (token = 1) -> 1: (token' = 2);

[East] (token = 2) -> 1: (token' = 3);
[West] (token = 2) -> 1: (token' = 3);
[North] (token = 2) -> 1: (token' = 3);
[South] (token = 2) -> 1: (token' = 3);

[client0] (token = 3) -> 1: (token' = 4);
[client1] (token = 4) -> 1: (token' = 5);

[updateFuel] (token = 5) -> 1: (token' = 6);

[updateDay] (token = 6) -> 1: (token' = 0);

endmodule


